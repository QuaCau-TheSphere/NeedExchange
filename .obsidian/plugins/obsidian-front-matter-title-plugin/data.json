{
  "version": "3.13.1",
  "templates": {
    "common": {
      "main": "title",
      "fallback": "fallback title"
    }
  },
  "rules": {
    "paths": {
      "mode": "black",
      "values": []
    },
    "delimiter": {
      "enabled": false,
      "value": ""
    }
  },
  "debug": false,
  "boot": {
    "delay": 5000,
    "background": true
  },
  "features": {
    "alias": {
      "enabled": true,
      "strategy": "ensure",
      "validator": "fr",
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "explorer": {
      "enabled": false,
      "sort": false,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "tab": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "header": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "graph": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "bookmarks": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "search": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "suggest": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "inlineTitle": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "canvas": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "backlink": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "windowFrame": {
      "enabled": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    },
    "noteLink": {
      "enabled": false,
      "strategy": "onlyEmpty",
      "approval": true,
      "templates": {
        "main": "",
        "fallback": ""
      }
    }
  },
  "processor": {
    "args": [
      "if (title.slice(-1) === \"？\") return title.replace(/？$/g, \"?\")"
    ],
    "type": "function"
  }
}
